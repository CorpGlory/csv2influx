{
  "influxdbUrl": "http://127.0.0.1:8086/traffic-violations",
  "measurementName": "events",
  "mapping": {
    "time": {
      "from": ["Date Of Stop", "Time Of Stop"],
      "type": "timestamp",
      "format": "jsDate"
    },
    "fieldSchema": {
      "coordinates": {
        "from": "${Latitude},${Longitude}",
        "type": "string"
      }
    },
    "tagSchema": {
      "agency": {
        "from": "Agency",
        "type": "*"
      }
    }
  },
  "csv": {
    "delimiter": ","
  }
}